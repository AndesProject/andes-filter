{
  "name": "@deltabit/filter",
  "version": "0.1.0",
  "private": false,
  "description": "Advanced TypeScript filtering library with Prisma-like object-based syntax. Supports complex queries with logical operators (AND/OR/NOT), comparison operators, array operations, date filtering, pagination, sorting, and MCP integration. Type-safe, high-performance, and optimized for large datasets.",
  "keywords": [
    "typescript",
    "filter",
    "query",
    "data-filtering",
    "search",
    "pagination",
    "sorting",
    "logical-operators",
    "comparison-operators",
    "array-operations",
    "date-filtering",
    "type-safe",
    "prisma-like",
    "object-based-syntax",
    "mcp",
    "model-context-protocol",
    "data-processing",
    "collection-filtering",
    "frontend",
    "backend",
    "javascript",
    "high-performance",
    "scalable",
    "query-builder"
  ],
  "homepage": "https://github.com/deltabit/deltabit#readme",
  "bugs": {
    "url": "https://github.com/deltabit/deltabit/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deltabit/deltabit.git"
  },
  "license": "CC-BY-NC-4.0",
  "author": "David Narvaez <dnarvaez@unimayor.edu.co>",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "default": "./dist/index.esm.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "filter": "./bin/cli.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc --emitDeclarationOnly --project tsconfig.json && vite build",
    "commitlint": "commitlint --edit",
    "dev": "vite",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "mcp:server": "npx tsx mcp/server.ts",
    "prepare": "husky",
    "prepublishOnly": "npm run build && npm run test",
    "start": "vite",
    "test": "npx vitest --coverage --run",
    "test:coverage": "npx vitest --coverage --run",
    "test:watch": "npx vitest --watch --coverage"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.22.0",
    "tsx": "^4.19.2",
    "yeoman-generator": "7.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/node": "^20.14.5",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-ts-react-important-stuff": "^3.0.0",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^9.0.11",
    "jsdom": "^24.1.1",
    "lint-staged": "^16.1.2",
    "prettier": "^3.5.3",
    "prettier-plugin-jsdoc": "^1.3.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.15",
    "typescript": "5.5.4",
    "vite": "^5.0.0",
    "vitest": "^3.1.1"
  },
  "engines": {
    "node": ">=24.11.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
